{
  "name": "HistYoury",
  "description": "Switch all fonts on everysite to Alef & click to make paragraphs blue.",
  "version": "1.0",

  "permissions": [
    "activeTab", "tabs", "<all_urls>","storage","browsingData"

  ],
  
  "browser_action": {
      "default_icon": "images/icon.jpg",           // optional
      "default_title": "HistYoury",      // optional; shown in tooltip
      "default_popup": "popup.html"        // optional
  },

  "omnibox": { "keyword" : "date" 
  },


  "content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["jquery-1.10.2.min.js","example.js","time.js"],
		"css": ["alef.css"]
  }],

  "manifest_version": 2
}