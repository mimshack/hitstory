{
  "name": "HitStory",
  "description": "save history pages hierarchy.",
  "version": "1.0",

"browser_action": {
       "default_icon": "images/icon-19.png",  
       "default_title": "HitStory",      // optional; shown in tooltip
       "default_popup": "panel.html"        // optional
  },
  "permissions": [
    "activeTab",
    "tabs" ,
    "storage",
    "browsingData",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
   "icons": {
    	"19": "images/icon-19.png",           // optional
	  	"48": "images/icon-48.png",
	  	"128": "images/icon-19.png"         // optional
  },
  "omnibox": { 
  	   "keyword" : "hitstory" 
  },
  "content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"js": ["js/jquery-1.10.2.min.js","js/underscore.min.js","js/example.js"],
		"css": ["css/main.css"]
  }],
  "manifest_version": 2
}