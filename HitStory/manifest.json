{
    "name": "HitStory",
    "description": "save history pages hierarchy.",
    "version": "1.0",
    "web_accessible_resources": [
        "js/underscore.min.js",
        "js/d3.min.js",
        "js/d3.v3.min.js",
        "js/underscore-min.map"
    ],
    "browser_action": {
        "default_icon": "images/icon-19.png",
        "default_title": "HitStory"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "unlimitedStorage",
        "browsingData",
        "notifications",
        "http://*/*",
        "https://*/*",
        "<all_urls>",
        "background"
    ],
    "icons": {
        "19": "images/icon-19.png",
        "128": "images/icon-128.png"
    },
    "omnibox": {
        "keyword": "hitstory"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/jquery-1.10.2.min.js",
                "js/saveHistory.js",
                "js/popup.js",
                "js/d3-timeline.js"
            ],
            "css": [
                "css/page.css"
            ]
        }
    ],
    "manifest_version": 2
}